{
    "name": "falveylibrarytechnology/vuowma",
    "description": "Office365 Webhook Message Aggregator",
    "license": "GPL-2.0",
    "config": {
        "platform": {
            "php": "8.0.0"
        },
        "process-timeout": 0,
        "allow-plugins": {
            "laminas/laminas-component-installer": true
        }
    },
    "require": {
        "php": "^8.0",
        "composer/package-versions-deprecated": "^1.10.99",
        "doctrine/orm": "^2.10.2",
        "laminas/laminas-component-installer": "^3.0.1",
        "laminas/laminas-config-aggregator": "^1.2",
        "laminas/laminas-diactoros": "^2.3.0",
        "laminas/laminas-http": "^2.13",
        "laminas/laminas-servicemanager": "^3.4",
        "laminas/laminas-stdlib": "^3.2.1",
        "mezzio/mezzio": "^3.2.1",
        "mezzio/mezzio-helpers": "^5.3",
        "mezzio/mezzio-laminasrouter": "^3.0.1",
        "roave/psr-container-doctrine": "^3.1.0"
    },
    "require-dev": {
        "filp/whoops": "^2.7.1",
        "friendsofphp/php-cs-fixer": "3.15.1",
        "laminas/laminas-development-mode": "^3.2",
        "mezzio/mezzio-tooling": "^2.6",
        "phing/phing": "^2.17.0",
        "phpspec/prophecy": "^1.10.3",
        "phpunit/phpunit": "^8.5.8 || ^9.3.7",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.6.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/AppTest/"
        }
    }
}
